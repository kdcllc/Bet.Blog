{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "Logs/Bet.Blog.WebApp-log-{Date}.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "retainedFileCountLimit": 10,
          "buffered": false
        }
      }
    ],
    "MinimumLevel": "Debug"
  }
}
